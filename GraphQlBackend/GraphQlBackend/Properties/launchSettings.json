			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"GOOGLE_APPLICATION_CREDENTIALS": "./firebase-config.json"
			}
		},
		"IIS Express": {
			"commandName": "IISExpress",
			"launchBrowser": true,
			"launchUrl": "graphql",
